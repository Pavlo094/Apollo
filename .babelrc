{
  "presets": ["env", "react", "stage-3"],
  "plugins": [
    ["module-resolver", {
      "alias": {
        "AppComponents": "./src/Components",
        "AppScenes": "./src/Scenes",
        "AppAuth": "./src/Auth",
        "AppUtils": "./src/Utils",
        "AppServices": "./src/Services",
        "AppConstants": "./src/Constants",
        "AppImgs": "./src/Img",
        "AppRedux": "./src/redux",
        "AppStyles": "./src/SharedStyles"
      }
    }],
      ["transform-class-properties"],
      ["styled-components"]
    ]
}
